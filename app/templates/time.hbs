{{#if (not imported)}}
  <h2>Time Report Summary:</h2>
  {{x-file-input alt="Import Time Report" class="upload" multiple=false action=(action "process") accept="image/jpg"}}
  {{#if true}}
    &emsp;Debug&nbsp;{{#paper-checkbox value=debug onChange=(action (mut debug))}}{{/paper-checkbox}}
  {{/if}}
{{else}}
  <h4>Nexonia Timecard File Loaded</h4>
  Report Week:
  {{#paper-select
    getItems=onUserLoad
    label="Week"
    selected=searchWeek
    options=weekList
    onChange=(action (mut searchWeek))
  as |item|
  }}
    {{item}}
  {{/paper-select}}&emsp;
  Managers:
    {{#paper-select
      getItems=onUserLoad
      label="manager"
      selected=mgr
      options=mgrList
      onChange=(action (mut mgr))
    as |item|
    }}
      {{item}}
    {{/paper-select}}&emsp;
  Missing Only&nbsp;{{#paper-checkbox value=missingOnly onChange=(action (mut missingOnly))}}{{/paper-checkbox}}&nbsp;
  Hide Disabled&nbsp;{{#paper-checkbox value=hideDisabled onChange=(action (mut hideDisabled))}}{{/paper-checkbox}}&nbsp;
  YTD&nbsp;{{#paper-checkbox value=showYTD onChange=(action (mut showYTD))}}{{/paper-checkbox}}&nbsp;
  {{#if showYTD}}
    {{#if update}}
      <h2>Missing Time YTD Report&emsp; week: {{searchWeek}}</h2>
    {{else}}
      <h2>Timecard YTD Report&emsp; week: {{searchWeek}}</h2>
    {{/if}}
  {{else}}
    {{#if update}}
      <h2>Missing Time Weekly Report&emsp; week: {{searchWeek}}</h2>
    {{else}}
      <h2>Timecard Weekly Report&emsp; week: {{searchWeek}}</h2>
    {{/if}}
  {{/if}}
  <br>

  {{#if debug}}
    <b>Processed Data:</b><br><br>
    {{textarea value=processed rows="8" cols='150'}}<br><br>
  {{/if}}
  {{{report}}}
{{/if}}

